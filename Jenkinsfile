pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building from branch "develop"..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing from branch "develop"..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying from branch "develop"....'
            }
        }
    }
}

